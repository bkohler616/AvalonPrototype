                <li><a href="~/Views/Programs and Classes.cshtml">Programs and Classes</a></li>
                <li><a href="~/Views/About Us.cshtml">About us</a></li>

            </ul>
        </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
